{
    "name": "dedermus/config",
    "description": "Config extension for open-admin-core, thanks to z-song",
    "type": "library",
    "keywords": ["open-admin", "setting"],
    "homepage": "https://github.com/dedermus/config",
    "license": "MIT",
    "authors": [
        {
            "name": "dedermus",
            "email": "dedermus@yandex.ru"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": ">=7.0",
        "dedermus/open-admin-core": "^v2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "^11"
    },
    "autoload": {
        "psr-4": {
            "OpenAdminCore\\Admin\\Config\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenAdminCore\\Admin\\Config\\ConfigServiceProvider"
            ]
        }
    }
}
